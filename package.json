{
  "name": "yugioh-bulmascores",
  "version": "1.0.0",
  "description": "bulma for Wordpress",
  "scripts": {
    "compile:sass": "node-sass --output-style expanded ./assets/sass/bulmascores.sass -o ./assets/css",
    "compile:postcss": "postcss -c postcss.config.js ./assets/css/*.css -r ./assets/css/",
    "build:css": "compile:sass && compile:postcss",
    "clean:css": "rimraf ./assets/css/*.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yugioh-hack/Bulmascores.git"
  },
  "keywords": [
    "wordpress",
    "gulp"
  ],
  "author": "Domenico Majorana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yugioh-hack/Bulmascores/issues"
  },
  "homepage": "https://github.com/yugioh-hack/Bulmascores#readme",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-wp-pot": "^2.3.1",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.0.0",
    "parallelshell": "^3.0.2",
    "postcss-cli": "^6.1.3",
    "postcss-extend": "^1.0.5",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "pug": "^2.0.4",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.3",
    "sanitize.css": "^11.0.0"
  },
  "dependencies": {},
  "optionalDependencies": {}
}
